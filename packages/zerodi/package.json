{
	"name": "zerodi",
	"version": "0.1.1",
	"description": "Lightweight, fast, fully typed dependency injector with compile-time generated code",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"bin": {
		"zerodi": "./dist/cli.js"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": ["dist"],
	"scripts": {
		"build": "tsup",
		"clean": "rm -rf dist"
	},
	"keywords": ["zerodi", "dependency injector", "code-gen"],
	"author": "hoffme",
	"license": "MIT",
	"devDependencies": {
		"tsup": "^8.0.1",
		"typescript": "^5.3.3"
	},
	"peerDependencies": {
		"typescript": "^5.0.0"
	}
}
