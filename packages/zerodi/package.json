{
	"name": "zerodi",
	"version": "0.1.0",
	"type": "module",
	"bin": {
		"zerodi": "./dist/cli.js"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
