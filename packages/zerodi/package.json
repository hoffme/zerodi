		"test:coverage": "vitest --coverage"
	},
	"devDependencies": {
		"@types/node": "^20.11.0",
		"tsup": "^8.0.1",
		"typescript": "^5.3.3",
		"vitest": "^2.1.0",
		"@vitest/ui": "^2.1.0",
		"@vitest/coverage-v8": "^2.1.0"
	},
	"dependencies": {
		"typescript": "^5.3.3"
	}
}
